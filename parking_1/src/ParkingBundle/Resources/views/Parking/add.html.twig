{% extends "base.html.twig" %}

{% block title %}Parking | Add{% endblock %}

{% block content_body %}
<div class="row">
    <div class="col-md-12">
        <div class="k-portlet"> 
            <div class="k-portlet__head">
            <div class="k-portlet__head-label">
                <h3 class="k-portlet__head-label">{{'Add parking > %parking_name%'|trans(parking_name is defined ?{'%parking_name%':parking_name}:'')}} </h3> 
            </div>
        </div>

            {{ form_start(form,{attr: {novalidate:'novalidate'}})}}
                {{ form_widget(form) }}
            {{ form_end(form) }}
        </div>
    </div>
</div>
{% endblock %}

